    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ------------------------------------------------------------------------------------------------------------
            // |  `    |  1  |  2  |  3   |  4   |  5   |                   |  6   |  7    |  8    |  9   |   0   |       |
            // |  ESC  |  Q  |  W  |  E   |  R   |  T   |                   |  Y   |  U    |  I    |  O   |   P   | BKSPC |
            // |  TAB  |  A  |  S  |  D   |  F   |  G   |                   |  H   |  J    |  K    |  L   |   ;   |   '   |
            // | SHIFT |  Z  |  X  |  C   |  V   |  B   |  MUTE  |  |       |  N   |  M    |  ,    |  .   |   /   | SHIFT |
            //               | GUI | ALT  | CTRL | LOWER|  ENTER |  | SPACE | RAISE| CTRL  | ALT   | GUI  |

            bindings = <
&kp NUMBER_1    &kp Q         &kp R                 &kp Q           &kp Q             &kp Q                                            &kp N6  &kp N7     &kp N8     &kp N9    &kp N0    &none
&bt BT_CLR_ALL  &kp W         &kp T                 &none           &out OUT_TOG      &kp C_BRIGHTNESS_INC                             &kp Y   &kp U      &kp I      &kp O     &kp P     &kp BSPC
&bt BT_SEL 0    &kp E         &kp U                 &none           &none             &kp C_BRIGHTNESS_DEC                             &kp H   &kp J      &kp K      &kp L     &kp SEMI  &kp SQT
&out OUT_USB    &out OUT_BLE  &kp I                 &kp C_PREVIOUS  &kp C_PLAY_PAUSE  &kp C_NEXT            &kp C_MUTE      &none      &kp N   &kp M      &kp COMMA  &kp DOT   &kp FSLH  &kp RSHFT
                              &kp LS(LEFT_CONTROL)  &kp LEFT_SHIFT  &kp LCTRL         &out OUT_USB          &kp LEFT_GUI    &kp SPACE  &mo 2   &kp RCTRL  &kp RALT   &kp RGUI
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOLUME_UP C_VOLUME_DOWN>,
                <&inc_dec_kp PG_UP PG_DN>;
        };

        lower_layer {
            // TODO: Some binds are waiting for shifted keycode support.
            // ------------------------------------------------------------------------------------------------------------
            // |       |  F1 |  F2 |  F3  |  F4  |  F5  |                   |  F6  |  F7   |  F8   |  F9  |  F10  |  F11  |
            // | `     |   1 |   2 |   3  |   4  |   5  |                   |   6  |   7   |   8   |   9  |    0  |  F12  |
            // |       |  !  |  @  |  #   |  $   |  %   |                   |  ^   |  &    |  *    |  (   |   )   |   |   |
            // |       |  =  |  -  |  +   |  {   |  }   |        |  |       |  [   |  ]    |  ;    |  :   |   \   |       |
            //               |     |      |      |      |        |  |       |      |       |       |      |

            bindings = <
&to 0   &to 5   &to 4   &to 6   &to 7   &to 10                    &kp F6     &kp F7    &kp F8           &kp F9     &kp F10   &kp F11
&to 11  &to 12  &to 9   &none   &none   &none                     &kp N6     &kp N7    &kp N8           &kp N9     &kp N0    &kp F12
&trans  &none   &none   &none   &none   &none                     &kp CARET  &kp AMPS  &kp KP_MULTIPLY  &kp LPAR   &kp RPAR  &kp PIPE
&trans  &none   &none   &none   &none   &none   &trans    &trans  &kp LBKT   &kp RBKT  &kp SEMI         &kp COLON  &kp BSLH  &trans
                &trans  &trans  &trans  &none   &trans    &trans  &trans     &trans    &trans           &trans
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOLUME_DOWN C_VOLUME_UP>,
                <&inc_dec_kp PG_UP PG_DN>;
        };

        Bluetooth {
            // ------------------------------------------------------------------------------------------------------------
            // |BTCLR| BT1  | BT2  |  BT3  |  BT4  |  BT5 |                |      |      |       |      |       |       |
            // |     | INS  | PSCR | GUI   |       |      |                | PGUP |      |       |      |       |       |
            // |     | ALT  | CTRL | SHIFT |       | CAPS |                |  <-  |   v  |   ^   |  ->  |  DEL  | BKSPC |
            // |     | UNDO | CUT  | COPY  | PASTE |      |      |  |      |      |      |       |      |       |       |
            //              |      |       |       |      |      |  |      |      |      |       |      |

            bindings = <
&bt BT_SEL 0  &bt BT_SEL 1  &bt BT_SEL 2  &bt BT_SEL 3  &bt BT_SEL 4  &bt BT_CLR                    &trans     &trans    &trans  &trans     &trans   &trans
&out OUT_BLE  &out OUT_TOG  &out OUT_USB  &none         &none         &none                         &kp PG_UP  &trans    &trans  &trans     &kp N0   &trans
&to 1         &to 1         &none         &none         &trans        &none                         &kp LEFT   &kp DOWN  &kp UP  &kp RIGHT  &kp DEL  &kp BSPC
&to 0         &kp K_UNDO    &none         &none         &none         &none       &trans    &trans  &trans     &trans    &trans  &trans     &trans   &trans
                            &none         &none         &none         &none       &none     &trans  &trans     &trans    &trans  &trans
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_UP C_VOL_DN &inc_dec_kp PG_UP PG_DN>;
        };

        Indian_Express {
            bindings = <
&dt_1  &dt_2  &kp G             &kp H               &IPAD_Prodrafts       &to 0                                   &trans  &trans  &trans  &trans  &trans  &trans
&dt_3  &dt_4  &kp Q             &kp W               &kp E                 &kp R                                   &trans  &trans  &trans  &trans  &trans  &trans
&dt_5  &dt_6  &kp A             &kp S               &kp D                 &kp F                                   &trans  &trans  &trans  &trans  &trans  &trans
&dt_7  &dt_8  &kp Z             &kp X               &kp C                 &kp V           &trans          &trans  &trans  &trans  &trans  &trans  &trans  &trans
              &kp LEFT_CONTROL  &mt LEFT_ALT LG(C)  &kp LS(LEFT_CONTROL)  &kp LEFT_SHIFT  &kp LEFT_GUI    &trans  &trans  &trans  &trans  &trans
